{
    "name": "gesu-shell",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "main": "electron/main.js",
    "scripts": {
        "dev": "vite",
        "dev:electron": "concurrently \"pnpm dev\" \"wait-on http://localhost:5173 && pnpm exec electron .\"",
        "dev:watch": "concurrently \"pnpm dev\" \"wait-on http://localhost:5173 && nodemon --watch electron --ext js,cjs,json --exec .\\scripts\\run-electron.cmd\"",
        "build": "tsc && vite build",
        "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
        "preview": "vite preview"
    },
    "dependencies": {
        "@dnd-kit/core": "^6.3.1",
        "@dnd-kit/sortable": "^10.0.0",
        "@dnd-kit/utilities": "^3.2.2",
        "@react-pdf/renderer": "^4.3.2",
        "@types/canvas-confetti": "^1.9.0",
        "@types/howler": "^2.2.12",
        "canvas-confetti": "^1.9.4",
        "echarts": "^6.0.0",
        "echarts-for-react": "^3.0.5",
        "electron-store": "^11.0.2",
        "framer-motion": "^12.23.26",
        "howler": "^2.2.4",
        "i18next": "^25.7.3",
        "i18next-browser-languagedetector": "^8.2.0",
        "lottie-react": "^2.4.1",
        "lucide-react": "^0.469.0",
        "react": "^18.2.0",
        "react-activity-calendar": "^3.0.4",
        "react-dom": "^18.2.0",
        "react-i18next": "^16.5.0",
        "react-router-dom": "^7.10.1",
        "react-spring": "^10.0.3",
        "sql.js": "^1.13.0",
        "swiper": "^12.0.3",
        "zod": "^4.2.1",
        "zustand": "^5.0.9"
    },
    "devDependencies": {
        "@types/react": "^18.2.43",
        "@types/react-dom": "^18.2.17",
        "@typescript-eslint/eslint-plugin": "^6.14.0",
        "@typescript-eslint/parser": "^6.14.0",
        "@vitejs/plugin-react": "^4.2.1",
        "autoprefixer": "^10.4.16",
        "concurrently": "^8.2.2",
        "electron": "^28.0.0",
        "eslint": "^8.55.0",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-react-refresh": "^0.4.5",
        "nodemon": "^3.0.0",
        "postcss": "^8.4.32",
        "tailwindcss": "^3.4.0",
        "typescript": "^5.9.3",
        "vite": "^5.0.8",
        "wait-on": "^7.2.0"
    }
}